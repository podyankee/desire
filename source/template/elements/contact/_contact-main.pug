main.main.gallery-main
	+breadcrumbs('Contact')
	.map
		iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d25220.92085336611!2d-122.42859046190706!3d37.79905897975507!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x808580f18d8c6ea7%3A0x5d7f53fb70a3a903!2zNzI1IEdyZWVuIFN0LCBTYW4gRnJhbmNpc2NvLCBDQSA5NDEzMywg0KHQqNCQ!5e0!3m2!1sru!2sua!4v1618962239558!5m2!1sru!2sua" width="100%" height="578" style="border:0;" allowfullscreen="" loading="lazy")
	section.contact
		.container
			h3.contact__title Contact Us
			ul.contact__social
				li.contact__social-item
					a(href="#").contact__social-link
						+img('icons/facebook.svg', 'contact__social-icon', 'facebook')
				li.contact__social-item
					a(href="#").contact__social-link
						+img('icons/instagram.svg', 'contact__social-icon', 'instagram')
				li.contact__social-item
					a(href="#").contact__social-link
						+img('icons/pinterest.svg', 'contact__social-icon', 'pinterest')
				li.contact__social-item
					a(href="#").contact__social-link
						+img('icons/whatsapp.svg', 'contact__social-icon', 'whatsapp')
				li.contact__social-item
					a(href="#").contact__social-link
						+img('icons/youtube.svg', 'contact__social-icon', 'youtube')
			.contact__box
				p.contact__text Vexillologist vape microdosing freegan pork belly deep v direct trade cray single-origin coffee street art. Viral shaman mustache master cleanse, pour-over affogato poutine copper mug marfa fanny pack normcore. Lo-fi pop-up banjo portland, echo park hammock
				a(href="tel:+14158297934").contact__phone (415) 829-7934
				.contact__st 725 Green St
				.contact__city San Francisco, California(CA), 94133
				a.info-mail(href="mailto:ouremailaddress@email.com") ouremailaddress@email.com
			form.contact__form
				input(type="text", placeholder="Your name").contact__form-input
				input(type="text", placeholder="Your t-mail").contact__form-input
				textarea(placeholder="Your message").contact__form-area
				button(type="submit").contact__form-btn Send
